<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Register">

    <EditText
        android:id="@+id/CpswrdTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="184dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/PswrdTxt"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/PswrdTxt" />

    <TextView
        android:id="@+id/Cpswrd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Confrm Password"
        app:layout_constraintBottom_toBottomOf="@+id/CpswrdTxt"
        app:layout_constraintEnd_toStartOf="@+id/CpswrdTxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/CpswrdTxt" />

    <TextView
        android:id="@+id/Pswrd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        app:layout_constraintBottom_toBottomOf="@+id/PswrdTxt"
        app:layout_constraintEnd_toStartOf="@+id/PswrdTxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/PswrdTxt" />

    <EditText
        android:id="@+id/CemailTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="76dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/PswrdTxt"
        app:layout_constraintEnd_toEndOf="@+id/EmailTxt"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/EmailTxt" />

    <TextView
        android:id="@+id/Cemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Confirm Email"
        app:layout_constraintBottom_toBottomOf="@+id/CemailTxt"
        app:layout_constraintEnd_toStartOf="@+id/CemailTxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/CemailTxt" />

    <TextView
        android:id="@+id/Email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email "
        app:layout_constraintBottom_toBottomOf="@+id/EmailTxt"
        app:layout_constraintEnd_toStartOf="@+id/EmailTxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/EmailTxt" />

    <EditText
        android:id="@+id/EmailTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="84dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/CemailTxt"
        app:layout_constraintStart_toStartOf="@+id/LnameTxt" />

    <EditText
        android:id="@+id/FnameTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toEndOf="@+id/Fname"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Lname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="Last Name"
        app:layout_constraintBottom_toBottomOf="@+id/LnameTxt"
        app:layout_constraintEnd_toStartOf="@+id/LnameTxt"
        app:layout_constraintHorizontal_bias="0.169"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/LnameTxt"
        app:layout_constraintVertical_bias="0.653" />

    <EditText
        android:id="@+id/LnameTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/EmailTxt"
        app:layout_constraintEnd_toEndOf="@+id/FnameTxt"
        app:layout_constraintStart_toStartOf="@+id/FnameTxt"
        app:layout_constraintTop_toBottomOf="@+id/FnameTxt"
        app:layout_constraintVertical_bias="0.361" />

    <TextView
        android:id="@+id/Fname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="First Name"
        app:layout_constraintBottom_toBottomOf="@+id/FnameTxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/FnameTxt"
        app:layout_constraintVertical_bias="0.384" />

    <EditText
        android:id="@+id/PswrdTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="52dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toTopOf="@+id/CpswrdTxt"
        app:layout_constraintEnd_toEndOf="@+id/CemailTxt"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/CemailTxt" />

    <Button
        android:id="@+id/Register"
        android:layout_width="169dp"
        android:layout_height="51dp"
        android:text="Register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CpswrdTxt" />
</androidx.constraintlayout.widget.ConstraintLayout>